<?xml version="1.0" encoding="UTF-8"?>
<project name="eve-logmon" default="build" basedir=".">

  <property name="source" location="src"/>
  <property name="build" location="build"/>

  <target name="build">
    <tstamp/>
    <mkdir dir="${build}"/>
    <javac srcdir="${source}" destdir="${build}"/>
  </target>

  <target name="clean">
    <delete dir="${build}"/>
  </target>

</project>
